{% extends 'base.html.twig' %}

{% block title %}{{ 'title.addcsv'|trans }}{% endblock %}

{% block body %}
    {% form_theme form with ['bootstrap_4_horizontal_layout.html.twig', 'form/form_errors.html.twig'] %}

    <h1>{{ 'title.addcsv'|trans }}</h1>

        {{ form_start(form, { method: 'post', action: url('addCsv') }) }}
        {{ form_widget(form, {'attr': { 'class': 'form-labels-on-top' }})  }}
        <input type="submit" value="{{ 'action.save'|trans }}" class="btn btn-success" />
        {{ form_end(form) }}
{% endblock %}